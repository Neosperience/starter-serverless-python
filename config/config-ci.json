{
    "logging": {
        "version": 1,
        "formatters": {
            "simple": {
                "format": "%(asctime)s %(name)s [%(levelname)s]: %(message)s"
            }
        },
        "handlers": {
            "console": {
                "level": "INFO",
                "class": "logging.StreamHandler",
                "formatter": "simple"
            }
        },
        "root": {
            "level": "INFO",
            "handlers": ["console"]
        }
    }
}
